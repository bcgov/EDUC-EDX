{
  "name": "edx",
  "version": "1.0.0",
  "private": true,
  "description": "EDX UI.",
  "scripts": {
    "serve": "vite --host",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "local:e2e": "testcafe chrome ./tests/e2e/testCases",
    "local:e2e:headless": "testcafe chrome:headless ./tests/e2e/testCases",
    "local:e2e:mobile": "testcafe chrome:emulation:device=iphone 11 ./tests/e2e/testCases",
    "test:e2e": "testcafe chrome ./tests/e2e/test.ts",
    "lint:fix": "vue-cli-service lint --fix",
    "lint": "vue-cli-service lint --no-fix",
    "purge": "rm -rf node_modules",
    "rebuild": "npm run clean && npm run build",
    "reinstall": "npm run purge && npm install",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^6.0.2",
    "@fortawesome/fontawesome-free": "6.3.0",
    "@js-joda/core": "^5.5.2",
    "@mdi/font": "^7.1.96",
    "axios": "^1.3.4",
    "core-js": "^3.28.0",
    "cron": "^2.2.0",
    "cryptico-js": "^1.1.0",
    "dotenv": "^16.0.3",
    "glob-all": "^3.3.1",
    "http-status-codes": "^2.2.0",
    "jest-junit": "^15.0.0",
    "minimist": "^1.2.8",
    "moment": "^2.29.4",
    "nconf": "^0.12.0",
    "path": "^0.12.7",
    "prom-client": "^14.1.1",
    "purgecss-webpack-plugin": "^5.0.0",
    "regenerator-runtime": "^0.13.11",
    "rfdc": "^1.3.0",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "testcafe": "^2.2.0",
    "v-viewer": "^1.6.4",
    "vue": "^3.2.47",
    "vue-clipboard2": "^0.3.3",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-quick-chat": "^1.2.8",
    "vue-router": "^4.1.6",
    "vuetify": "~3.1.6",
    "vite": "^4.1.4",
    "pinia": "^2.0.32"
  },
  "devDependencies": {
    "axios-mock-adapter": "1.21.2",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.9.0",
    "flush-promises": "^1.0.2",
    "jest": "^29.4.3",
    "jest-sonar-reporter": "^2.0.0",
    "jest-transform-stub": "^2.0.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.21",
    "stylus": "^0.59.0",
    "stylus-loader": "^7.1.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "vue-template-compiler": "^2.7.14"
  },
  "browserslist": [
    "> 1%",
    "last 20 versions",
    "not ie <= 8"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly",
      "_": false
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module",
      "ecmaVersion": 8
    },
    "rules": {
      "vue/multi-word-component-names": 0,
      "vue/no-v-text-v-html-on-component": 0,
      "vue/no-mutating-props": 0,
      "vue/no-unused-vars": 0,
      "vue/valid-v-slot": 0,
      "vue/no-reserved-component-names": 0,
      "no-console": "off",
      "eol-last": [
        "error",
        "always"
      ],
      "indent": [
        "error",
        2
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "clearMocks": true,
    "setupFiles": [
      "<rootDir>/node_modules/regenerator-runtime/runtime.js",
      "<rootDir>/tests/unit/setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest",
      ".*\\.(vue)$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!vue-loading-spinner)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^.+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/main.js",
      "!src/plugins/*.*",
      "!src/router.js",
      "!tests/e2e/student-form.js",
      "!src/common/staticConfig.js"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "coverage"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  }
}
